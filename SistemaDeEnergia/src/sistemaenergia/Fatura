package sistemaenergia;

public class Fatura extends tarifa {
      
      private int faruraid;

      private int mesAno;

      private UC unidadeConsumidora;

      private  int consumo;

      private boolean pago;

	public int getFaruraid() {
		return faruraid;
	}

	public void setFaruraid(int faruraid) {
		this.faruraid = faruraid;
	}

	public int getMesAno() {
		return mesAno;
	}

	public void setMesAno(int mesAno) {
		this.mesAno = mesAno;
	}

	public UC getUnidadeConsumidora() {
		return unidadeConsumidora;
	}

	public void setUnidadeConsumidora(UC unidadeConsumidora) {
		this.unidadeConsumidora = unidadeConsumidora;
	}

	public int getConsumo() {
		return consumo;
	}

	public void setConsumo(int consumo) {
		this.consumo = consumo;
	}

	public boolean isPago() {
		return pago;
	}

	public void setPago(boolean pago) {
		this.pago = pago;
	}

public double  calculoValorFatura(){
      
      private double subTotal=0;

    if(consumo <=200){

            subTotal=(this.consumo*TarReciddencia2)+getImcs+getTaxailuminaçãoPu;



      }  else if (consumo >200 && consumo<=500){

            subTotal=(this.consumo* getTarComercial2)+getImcs+getTaxailuminaçãoPu;


      } else{
           System.out.println(".o.consumo.não.é.valido.");

	  }


}
 public String toString() {
	String texto = "";
		
		texto = "----------------------------"
				+"\n Rua: " + getFaruraid()
				+"\n Rua: " + getMesAno()
				+"\n CEP: " + getUnidadeConsumidora()
				+"\n Cidade: " + getConsumo();
		
		return texto;
				
	
	}

      public void imprimirFatura(Fatura Vert[]){





            


      }


      

}